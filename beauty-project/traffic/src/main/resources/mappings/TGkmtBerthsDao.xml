    <!--获取总个数-->
    <select id="getTGkmtBerthsCount" resultType="java.lang.Integer">
        SELECT count(1) FROM T_GKMT_BERTHS
    </select>
    <!--获取总个数-->
    <select id="getTGkmtBerthsCountByParams" resultType="java.lang.Integer">
        SELECT count(1) FROM T_GKMT_BERTHS
    </select>

    <select id="getTGkmtBerthsByPageable" resultMap="TGkmtBerthsInfoResultMap">
      SELECT * 
      FROM (SELECT ROWNUM as rn, a.* from T_GKMT_BERTHS a WHERE ROWNUM &lt;= #{end}) WHERE rn &gt; #{start}
    </select>
    <select id="getTGkmtBerthsByParamsPageable" resultMap="TGkmtBerthsInfoResultMap">
      SELECT * 
      FROM (SELECT ROWNUM as rn, a.* from T_GKMT_BERTHS a WHERE ROWNUM &lt;= #{end}) WHERE rn &gt; #{start}
    </select>
    <select id="getHomepageInfo" resultMap="homepageInfoResultMap">
      select '***' AS name, '***' AS second_name, count(DISTINCT ***) as second_value, 
      '***' as third_name, COUNT(DISTINCT ***) as third_value, '***' as fourth_name, 
      COUNT(DISTINCT ***) as fourth_value from T_GKMT_BERTHS
    </select>
