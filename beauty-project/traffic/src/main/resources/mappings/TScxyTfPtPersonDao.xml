    </select>
    <!--获取总个数-->
    <select id="getTScxyTfPtPersonCountByParams" resultType="java.lang.Integer">
        SELECT count(1) FROM CDPRJ.T_SCXY_TF_PT_PERSON a 
      WHERE (a.NAME like CONCAT('%', CONCAT(#{info},'%'))
      or a.CARDNO like CONCAT('%', CONCAT(#{info},'%')))
    </select>

