"SUM"(DLYS12005N0000010) as DLYS12005N0000010,
"SUM"(DLYS12005N0000200) as DLYS12005N0000200
FROM CDPRJ.T_TJJH_DLSY005N_GD
where "CONCAT"("SUBSTR"(DATATIME,1,4), "SUBSTR"(DATATIME,8,2)) BETWEEN #{start} AND #{end}
